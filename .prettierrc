{
  "arrowParens": "always",
  "proseWrap": "always",
  "tabWidth": 4,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^[../]",
    "^[./]",
    "<THIRD_PARTY_TS_TYPES>",
    "<TS_TYPES>"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["prettier-plugin-jsdoc"]
}
