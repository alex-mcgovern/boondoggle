    "clsx": "1.2.1",
    "downshift": "7.6.0",
    "react-hook-form": "7.43.9",
    "zod": "3.21.4"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
