{
	"author": "Alex McGovern",
	"name": "boondoggle",
	"description": "Boondoggle component library",
	"version": "2.1.9",
	"license": "MIT",
	"sideEffects": ["**/*.css"],
	"files": ["dist"],
	"repository": {
		"type": "git",
		"url": "https://github.com/alex-mcgovern/boondoggle"
	},
	"source": "src/index.ts",
	"types": "dist/types.d.ts",
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"biome": "biome ci",
		"biome:fix": "biome check . --write && biome format . --write",
		"build": "parcel build --no-cache",
		"build:docs": "storybook build --docs -o docs",
		"check": "concurrently -g -n BIOME,TSC,VITEST,KNIP -c \"bgYellow.bold,bgBlue.bold,bgRed.bold,bgGreen.bold\" \"npm:biome\" \"npm:tsc\" \"npm:vitest\" \"npm:knip\"",
		"cy:open": "cypress open",
		"cy:run": "cypress run --component",
		"knip": "knip",
		"postinstall": "npm run biome:fix",
		"postversion": "npm run biome:fix",
		"prepare": "husky",
		"storybook": "storybook dev -p 6006",
		"test:watch": "vitest --watch",
		"tsc": "tsc",
		"vitest": "vitest run --passWithNoTests"
	},
	"peerDependencies": {
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "6.5.1",
		"@fortawesome/pro-duotone-svg-icons": "6.5.1",
		"@fortawesome/pro-solid-svg-icons": "6.5.1",
		"@fortawesome/react-fontawesome": "0.2.0",
		"@internationalized/date": "3.5.4",
		"@radix-ui/colors": "3.0.0",
		"@radix-ui/react-collapsible": "1.1.0",
		"clsx": "2.1.1",
		"parcel": "2.12.0",
		"react-aria-components": "1.2.1",
		"react-hook-form": "7.52.0",
		"sonner": "1.3.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.2",
		"@faker-js/faker": "8.4.1",
		"@parcel/packager-ts": "2.12.0",
		"@parcel/transformer-typescript-tsc": "2.12.0",
		"@parcel/transformer-typescript-types": "2.12.0",
		"@storybook/addon-essentials": "8.1.10",
		"@storybook/blocks": "8.1.10",
		"@storybook/manager-api": "8.1.10",
		"@storybook/react": "8.1.10",
		"@storybook/react-vite": "8.1.10",
		"@storybook/theming": "8.1.10",
		"@testing-library/jest-dom": "6.4.6",
		"@testing-library/react": "16.0.0",
		"@testing-library/user-event": "14.5.2",
		"@types/react": "18.3.3",
		"@types/react-dom": "18.3.0",
		"@vitejs/plugin-react-swc": "3.7.0",
		"concurrently": "8.2.2",
		"cypress": "13.12.0",
		"husky": "9.0.11",
		"jsdom": "24.1.0",
		"knip": "5.23.0",
		"react": "^18.3.1",
		"storybook": "8.1.10",
		"tslib": "2.6.3",
		"typescript": "5.5.2",
		"vite": "5.3.1",
		"vitest": "1.6.0",
		"vitest-fail-on-console": "0.7.0"
	},
	"optionalDependencies": {
		"@parcel/watcher-linux-x64-glibc": "2.4.2-alpha.0",
		"@rollup/rollup-linux-x64-gnu": "4.18.0",
		"lightningcss-linux-x64-gnu": "1.25.1"
	}
}
