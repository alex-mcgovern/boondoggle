{
  "author": "Alex McGovern",
  "name": "@alex-mcgovern/boondoggle.design",
  "description": "Boondoggle design system component library",
  "version": "0.8.122",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alex-mcgovern/boondoggle.design"
  },
  "sideEffects": [
    "*.css",
    "*.css.ts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build-storybook": "storybook build",
    "build:dev": "rm -rf lib && tsup --watch",
    "build:prod": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=8192' tsup",
    "build:watch": "tsup --watch",
    "check": "npm run tsc && npm run ci-lint && npm run test",
    "ci-build-prerelease": "npm run build:prod && npm version prerelease && npm publish --access public",
    "ci-lint": "eslint . --quiet",
    "format": "prettier  --loglevel warn --write \"./**/*.{ts,tsx,css,md,json}\" ",
    "lint:fix": "eslint . --quiet --fix",
    "patch": "npm run check && npm run build:prod && npm version patch && npm publish --access public",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "test:watch": "jest --watch",
    "tsc": "tsc"
  },
  "dependencies": {
    "@dessert-box/core": "0.2.0",
    "@dessert-box/react": "0.7.5",
    "@fortawesome/pro-light-svg-icons": "^6.4.0",
