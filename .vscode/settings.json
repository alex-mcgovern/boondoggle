{
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"commentBox.styles": {
		"h1": {
			"bottomEdgeToken": "-",
			"bottomLeftToken": "* ",
			"boxWidth": 80,
			"capitalize": true,
			"commentEndToken": "- */",
			"commentStartToken": "/** ",
			"leftEdgeToken": "* ",
			"rightEdgeToken": "",
			"textAlignment": "left",
			"topEdgeToken": "-",
			"topRightToken": "-"
		},
		"h1_JSX": {
			"bottomEdgeToken": "-",
			"bottomLeftToken": "* ",
			"boxWidth": 80,
			"capitalize": true,
			"commentEndToken": "- */}",
			"commentStartToken": "{/** ",
			"leftEdgeToken": "* ",
			"rightEdgeToken": "",
			"textAlignment": "left",
			"topEdgeToken": "-",
			"topRightToken": "-"
		},
		"h2": {
			"bottomEdgeToken": "-",
			"bottomLeftToken": "* ",
			"boxWidth": 48,
			"capitalize": true,
			"commentEndToken": "- */",
			"commentStartToken": "/** ",
			"leftEdgeToken": "* ",
			"rightEdgeToken": "",
			"textAlignment": "left",
			"topEdgeToken": "-",
			"topRightToken": "-"
		},
		"h2_JSX": {
			"bottomEdgeToken": "-",
			"bottomLeftToken": "* ",
			"boxWidth": 48,
			"capitalize": true,
			"commentEndToken": "- */}",
			"commentStartToken": "{/** ",
			"leftEdgeToken": "* ",
			"rightEdgeToken": "",
			"textAlignment": "left",
			"topEdgeToken": "-",
			"topRightToken": "-"
		}
	},
	"cSpell.language": "en-GB",
	"cSpell.words": [
		"arrowdown",
		"arrowright",
		"boondoggle",
		"Clearable",
		"clsx",
		"combobox",
		"Dismissable",
		"Hideable",
		"Lastpass",
		"popover",
		"Toggleable",
		"vars"
	],
	"git.ignoreLimitWarning": true,
	"mdx.experimentalLanguageServer": true,
	"search.exclude": {
		".git": true,
		"**/.cache": true,
		"**/*.snap": true,
		"**/node_modules": true,
		"**/package-lock.json": true,
		"build": true,
		"dist": true,
		"docs": true
	},
	"turboConsoleLog.delimiterInsideMessage": "",
	"turboConsoleLog.includeFileNameAndLineNum": false,
	"turboConsoleLog.insertEnclosingClass": false,
	"turboConsoleLog.insertEnclosingFunction": false,
	"turboConsoleLog.logMessagePrefix": "debug",
	"turboConsoleLog.logType": "debug"
}
